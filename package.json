{
	"name": "midnightjs",
	"version": "1.0.0",
	"description": "A simple NodeJS framework for the NodeJS Discord API wrapper Eris, which makes bot development easier for developers.",
	"main": "index.js",
	"types": "types",
	"scripts": {
		"lint": "eslint .",
		"start": "node test/index.js",
		"test": "npm test",
		"test:typescript": "npx tsc",
		"dtslint": "dtslint types",
		"prettier": "npx prettier --write . --use-tabs true",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Midnight-Development/MidnightJS.git"
	},
	"keywords": [
		"MidnightJS",
		"NodeJS",
		"Javascript",
		"Eris",
		"Framework",
		"Discord",
		"API"
	],
	"author": "Midnight Development <contact.midnightdev@gmail.com>",
	"contributors": [
		"Ascendus <ascendus.dev@gmail.com>",
		"lucaslah"
	],
	"maintainers": [
		"Ascendus <ascendus.dev@gmail.com>",
		"lucaslah"
	],
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/Midnight-Development/MidnightJS/issues"
	},
	"homepage": "https://github.com/Midnight-Development/MidnightJS#readme",
	"dependencies": {
		"@discordjs/collection": "^0.2.0",
		"@types/node": "^16.4.0",
		"eris": "^0.15.1",
		"events": "^3.3.0"
	},
	"devDependencies": {
		"dtslint": "^3.4.2",
		"eslint": "^7.31.0",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.0.1",
		"node-gyp": "^8.1.0",
		"prettier": "2.3.2",
		"tslint": "^6.1.3",
		"typescript": "^4.3.5"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
